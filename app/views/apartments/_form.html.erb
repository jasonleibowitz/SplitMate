<%= form_for @apartment do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :street %>
  <%= f.text_field :street %>

  <%= f.label :apt %>
  <%= f.text_field :apt %>

  <%= f.label :zipcode %>
  <%= f.number_field :zipcode %>

  <%= f.label 'Apartment Image' %>
  <%= f.file_field :avatar %>

  <%= f.submit :submit %>

<% end %>
